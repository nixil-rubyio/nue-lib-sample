    "new": "plop --plopfile ./scripts/plopfile.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "7.14.3",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "7.18.6",
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "babel-loader": "^8.2.2",
    "commitizen": "^4.1.2",
    "cpr": "^3.0.1",
    "cz-conventional-changelog": "^3.2.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "husky": "^4.2.5",
    "jest": "^27.2.0",
    "lint-staged": "^10.2.11",
    "plop": "^2.7.4",
    "prettier": "^2.0.5",
    "prismjs": "^1.21.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@types/inquirer": "^7.3.0",
    "@types/jest": "^29.2.4",
    "@types/semver": "^7.3.1",
    "babel-plugin-async-to-promises": "^1.0.5",
    "semver": "^7.3.8",
    "@nue-apps/nue-pricing-clientjs": "2023.1.1"
  },
  "lint-staged": {
    "src/**/*.ts?(x)": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "registry": "http://nexus.nuedev.click/repository/npm-private/"
  }
